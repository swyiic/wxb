{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "global-perms",
  "description": "给整个应用放开常用权限",
  "windows": ["*"],
  "permissions": [
    "core:default",
    "fs:default",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "**"
        }
      ]
    }
  ]
}